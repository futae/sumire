<%= form_for @plus_place, :url => {:action => 'plus_place_create'} do |f| %>
  <%= render 'layouts/error_messages', model: f.object %>

  <div class="form-group">
    <%= f.label :mymap_id, "追加するマイマップ" %>
    <%= f.collection_select :mymap_id, current_user.mymaps.all, :id, :name, :prompt => true %>
  </div>

  <%= f.submit "登録", class: 'btn btn-primary btn-block' %>
<% end %>
